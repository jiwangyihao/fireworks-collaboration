{
  "//": "=========================================================",
  "//": "  fireworks-collaboration Proxy Configuration Example",
  "//": "  P5.0 Phase: Baseline Configuration Model",
  "//": "  P5.1 Phase: HTTP/HTTPS Proxy Support (IMPLEMENTED)",
  "//": "  P5.2 Phase: SOCKS5 Proxy Support (IMPLEMENTED)",
  "//": "=========================================================",

  "//": "",
  "//": "This file demonstrates all available proxy configuration",
  "//": "options. Copy this file to config.json and customize as",
  "//": "needed for your environment.",
  "//": "",

  "proxy": {
    "//": "------- Basic Proxy Settings -------",
    "//": "",
    "//": "mode: Proxy mode selection",
    "//": "  - 'off': Disable proxy (default)",
    "//": "  - 'http': Use HTTP/HTTPS proxy",
    "//": "  - 'socks5': Use SOCKS5 proxy (P5.2)",
    "//": "  - 'system': Auto-detect system proxy settings",
    "mode": "http",

    "//": "",
    "//": "url: Proxy server URL",
    "//": "  Format: <scheme>://<host>:<port>",
    "//": "  Schemes: http://, https://, socks5://",
    "//": "  Examples:",
    "//": "    - http://proxy.company.com:8080",
    "//": "    - https://secure-proxy.example.com:443",
    "//": "    - socks5://localhost:1080",
    "//": "  Note: Credentials can be in URL or separate fields below",
    "url": "http://proxy.example.com:8080",

    "//": "",
    "//": "username/password: Proxy authentication (optional)",
    "//": "  If provided, these override credentials in URL",
    "//": "  Leave as null if proxy doesn't require authentication",
    "username": null,
    "password": null,

    "//": "",
    "//": "disableCustomTransport: Force disable custom transport",
    "//": "  - true: Always use libgit2's native transport",
    "//": "  - false: Let proxy settings control transport",
    "//": "  Note: Enabling proxy (mode != 'off') forces this to true",
    "//": "        to avoid conflicts with Fake SNI / IP optimization",
    "disableCustomTransport": false,

    "//": "",
    "//": "------- Connection Settings -------",
    "//": "",
    "//": "timeoutSeconds: Connection timeout in seconds",
    "//": "  Range: 1-300 seconds",
    "//": "  Default: 30 seconds",
    "//": "  Recommended: 30-60 for most networks",
    "timeoutSeconds": 30,

    "//": "",
    "//": "------- Automatic Fallback (P5.4) -------",
    "//": "",
    "//": "fallbackThreshold: Failure rate threshold for auto-fallback",
    "//": "  Range: 0.0-1.0 (0% to 100%)",
    "//": "  Default: 0.2 (20% failure rate)",
    "//": "  Example: 0.3 = fallback when 30% of attempts fail",
    "//": "  Note: Set to 1.0 to disable auto-fallback",
    "fallbackThreshold": 0.2,

    "//": "",
    "//": "fallbackWindowSeconds: Time window for failure tracking",
    "//": "  Range: 10-3600 seconds (10s to 1 hour)",
    "//": "  Default: 60 seconds",
    "//": "  Note: Failures older than this window are ignored",
    "fallbackWindowSeconds": 60,

    "//": "",
    "//": "------- Automatic Recovery (P5.5) -------",
    "//": "",
    "//": "healthCheckUrl: URL used for proxy health checks",
    "//": "  Default: https://www.google.com",
    "//": "  Note: Should be a reliable, globally accessible URL",
    "//": "  Used to test proxy connectivity during recovery",
    "healthCheckUrl": "https://www.google.com",

    "//": "",
    "//": "healthCheckIntervalSec: Interval between health checks",
    "//": "  Range: 10-3600 seconds (10s to 1 hour)",
    "//": "  Default: 60 seconds",
    "//": "  Note: Used during recovery phase (P5.5)",
    "healthCheckIntervalSec": 60,

    "//": "",
    "//": "healthCheckTimeoutSec: Timeout for each health check",
    "//": "  Range: 1-300 seconds",
    "//": "  Default: 10 seconds",
    "//": "  Note: Should be shorter than healthCheckIntervalSec",
    "healthCheckTimeoutSec": 10,

    "//": "",
    "//": "fallbackAfterFailures: Number of failures before fallback",
    "//": "  Range: 1-100",
    "//": "  Default: 3",
    "//": "  Note: Consecutive failures trigger automatic fallback",
    "fallbackAfterFailures": 3,

    "//": "",
    "//": "recoverAfterSuccesses: Successes needed for recovery",
    "//": "  Range: 1-100",
    "//": "  Default: 2",
    "//": "  Note: Consecutive health check successes to recover",
    "recoverAfterSuccesses": 2,

    "//": "",
    "//": "fallbackCooldownSec: Cooldown period before attempting recovery",
    "//": "  Range: 10-3600 seconds (10s to 1 hour)",
    "//": "  Default: 300 seconds (5 minutes)",
    "//": "  Note: Prevents rapid recovery attempts after fallback",
    "fallbackCooldownSec": 300,

    "//": "",
    "//": "------- Debug & Diagnostics (P5.6) -------",
    "//": "",
    "//": "debugProxyLogging: Enable verbose proxy logging",
    "//": "  - true: Log all proxy operations and state changes",
    "//": "  - false: Log only errors and important events (default)",
    "//": "  Note: Useful for troubleshooting proxy issues",
    "debugProxyLogging": false,

    "//": "",
    "//": "recoveryCooldownSeconds: Wait time before recovery attempts",
    "//": "  Range: 10-3600 seconds (10s to 1 hour)",
    "//": "  Default: 300 seconds (5 minutes)",
    "//": "  Note: Prevents rapid recovery attempts",
    "recoveryCooldownSeconds": 300,

    "//": "",
    "//": "recoveryStrategy: Strategy for proxy recovery",
    "//": "  Options:",
    "//": "    - 'immediate': Recover immediately after cooldown",
    "//": "    - 'consecutive': Require N consecutive successes",
    "//": "    - 'exponential-backoff': Gradual retry with backoff",
    "//": "  Default: 'consecutive'",
    "//": "  Note: Detailed implementation in P5.5",
    "recoveryStrategy": "consecutive"
  },

  "//": "",
  "//": "=========================================================",
  "//": "  P6.0 Phase: Credential Storage & Security Management",
  "//": "=========================================================",
  "//": "",
  "credential": {
    "//": "------- Storage Settings -------",
    "//": "",
    "//": "storage: Credential storage type",
    "//": "  Options:",
    "//": "    - 'system': Use system keychain (default)",
    "//": "      * macOS: Keychain",
    "//": "      * Windows: Credential Manager",
    "//": "      * Linux: Secret Service",
    "//": "    - 'file': Encrypted file storage (AES-256-GCM)",
    "//": "    - 'memory': Temporary in-memory storage (session only)",
    "//": "  Default: 'system'",
    "//": "  Note: System keychain fallback to file if unavailable",
    "storage": "system",

    "//": "",
    "//": "defaultTtlSeconds: Default credential expiration time",
    "//": "  Range: null (never expire) or 1-31536000 seconds (1 year)",
    "//": "  Default: 7776000 (90 days)",
    "//": "  Note: Individual credentials can override this",
    "defaultTtlSeconds": 7776000,

    "//": "",
    "//": "debugLogging: Enable verbose credential operation logs",
    "//": "  WARNING: May expose sensitive information in logs",
    "//": "  - true: Log all operations including masked credentials",
    "//": "  - false: Log only errors and important events (default)",
    "//": "  Default: false",
    "//": "  Recommendation: Only enable in development environment",
    "debugLogging": false,

    "//": "",
    "//": "auditMode: Enable audit logging with hash digests",
    "//": "  - true: Record SHA-256 hash of credentials in audit log",
    "//": "  - false: No audit logging (default)",
    "//": "  Default: false",
    "//": "  Note: Useful for compliance and security auditing",
    "auditMode": false,

    "//": "",
    "//": "requireConfirmation: Require user confirmation for operations",
    "//": "  - true: Prompt before accessing/modifying credentials",
    "//": "  - false: No confirmation required (default)",
    "//": "  Default: false",
    "//": "  Note: Future implementation (P6.3+)",
    "requireConfirmation": false,

    "//": "",
    "//": "filePath: Custom path for encrypted credential file",
    "//": "  Only used when storage = 'file'",
    "//": "  Default: null (use default location: ~/.config/fireworks/credentials.enc)",
    "//": "  Example: \"/path/to/custom/credentials.enc\"",
    "filePath": null,

    "//": "",
    "//": "keyCacheTtlSeconds: Master key cache duration",
    "//": "  Range: 60-86400 seconds (1 minute to 1 day)",
    "//": "  Default: 3600 (1 hour)",
    "//": "  Note: Cached key expires after this duration, requiring re-authentication",
    "keyCacheTtlSeconds": 3600
  },

  "//": "",
  "//": "=========================================================",
  "//": "  Credential Configuration Scenarios (P6.0)",
  "//": "=========================================================",
  "//": "",
  "//": "Scenario 1: Default System Keychain (Recommended)",
  "//": "{",
  "//": "  \"credential\": {",
  "//": "    \"storage\": \"system\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 2: Encrypted File with Custom Path",
  "//": "{",
  "//": "  \"credential\": {",
  "//": "    \"storage\": \"file\",",
  "//": "    \"filePath\": \"/secure/path/credentials.enc\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 3: High-Security with Audit Logging",
  "//": "{",
  "//": "  \"credential\": {",
  "//": "    \"storage\": \"system\",",
  "//": "    \"auditMode\": true,",
  "//": "    \"requireConfirmation\": true,",
  "//": "    \"defaultTtlSeconds\": 2592000",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 4: Temporary Session-Only Storage",
  "//": "{",
  "//": "  \"credential\": {",
  "//": "    \"storage\": \"memory\",",
  "//": "    \"defaultTtlSeconds\": null",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 5: Short-Lived Credentials (30 days)",
  "//": "{",
  "//": "  \"credential\": {",
  "//": "    \"storage\": \"system\",",
  "//": "    \"defaultTtlSeconds\": 2592000,",
  "//": "    \"keyCacheTtlSeconds\": 1800",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "=========================================================",
  "//": "  Security Considerations (P6.0)",
  "//": "=========================================================",
  "//": "",
  "//": "1. Storage Type Selection:",
  "//": "   - 'system': Most secure, uses OS-level encryption",
  "//": "   - 'file': Secure with master password, portable",
  "//": "   - 'memory': Least secure, use only for testing",
  "//": "",
  "//": "2. TTL Recommendations:",
  "//": "   - Production: 30-90 days (2592000-7776000 seconds)",
  "//": "   - Development: 7-30 days (604800-2592000 seconds)",
  "//": "   - High-security: 7-14 days (604800-1209600 seconds)",
  "//": "",
  "//": "3. Audit Mode:",
  "//": "   - Enable for compliance requirements (SOC 2, GDPR)",
  "//": "   - Records only hash digests, not plaintext",
  "//": "   - Minimal performance impact",
  "//": "",
  "//": "4. Key Cache TTL:",
  "//": "   - Longer TTL: Better UX, slightly lower security",
  "//": "   - Shorter TTL: Better security, more re-authentication",
  "//": "   - Recommended: 1-4 hours (3600-14400 seconds)",
  "//": "",
  "//": "5. File Storage Security:",
  "//": "   - Uses AES-256-GCM encryption",
  "//": "   - Master password required",
  "//": "   - File permissions set to 0600 (owner read/write only)",
  "//": "   - See CREDENTIAL_ENCRYPTION_DESIGN.md for details",
  "//": "",
  "//": "=========================================================",

  "//": "",
  "//": "=========================================================",
  "//": "  Common Configuration Scenarios",
  "//": "=========================================================",
  "//": "",
  "//": "Scenario 1: Corporate HTTP Proxy (No Auth) [P5.1 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"http\",",
  "//": "    \"url\": \"http://proxy.company.com:8080\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 2: HTTP Proxy with Basic Authentication [P5.1 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"http\",",
  "//": "    \"url\": \"http://proxy.company.com:8080\",",
  "//": "    \"username\": \"myuser\",",
  "//": "    \"password\": \"mypass\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 3: Auto-detect System Proxy [P5.0 READY - Detection Only]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"system\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 4: SOCKS5 Proxy (No Authentication) [P5.2 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"socks5\",",
  "//": "    \"url\": \"socks5://localhost:1080\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 5: SOCKS5 Proxy with Authentication [P5.2 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"socks5\",",
  "//": "    \"url\": \"socks5://proxy.company.com:1080\",",
  "//": "    \"username\": \"myuser\",",
  "//": "    \"password\": \"mypass\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 6: SOCKS5 URL Formats (All Supported) [P5.2 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"socks5\",",
  "//": "    \"url\": \"socks://localhost:1080\"",
  "//": "    // OR: \"url\": \"localhost:1080\" (scheme optional)",
  "//": "    // OR: \"url\": \"socks5://[::1]:1080\" (IPv6 supported)",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 7: Custom Timeout for Slow Networks [P5.1 READY]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"http\",",
  "//": "    \"url\": \"http://proxy.example.com:8080\",",
  "//": "    \"timeoutSeconds\": 60",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "Scenario 8: Aggressive Fallback for Unstable Proxy [P5.4 - NOT YET IMPLEMENTED]",
  "//": "{",
  "//": "  \"proxy\": {",
  "//": "    \"mode\": \"http\",",
  "//": "    \"url\": \"http://unreliable-proxy.example.com:8080\",",
  "//": "    \"fallbackThreshold\": 0.1,",
  "//": "    \"fallbackWindowSeconds\": 30,",
  "//": "    \"recoveryCooldownSeconds\": 600,",
  "//": "    \"recoveryStrategy\": \"exponential-backoff\"",
  "//": "  }",
  "//": "}",
  "//": "",
  "//": "=========================================================",
  "//": "  Validation Rules",
  "//": "=========================================================",
  "//": "",
  "//": "URL:",
  "//": "  - Must start with http://, https://, or socks5://",
  "//": "  - Port must be 1-65535 if specified",
  "//": "  - Cannot contain whitespace",
  "//": "",
  "//": "Timeouts:",
  "//": "  - timeoutSeconds: 1-300",
  "//": "  - fallbackWindowSeconds: 10-3600",
  "//": "  - recoveryCooldownSeconds: 10-3600",
  "//": "  - healthCheckIntervalSeconds: 10-3600",
  "//": "",
  "//": "Thresholds:",
  "//": "  - fallbackThreshold: 0.0-1.0",
  "//": "",
  "//": "Recovery Strategy:",
  "//": "  - Must be: immediate, consecutive, or exponential-backoff",
  "//": "",
  "//": "=========================================================",
  "//": "  Mutual Exclusion with Fake SNI (P0)",
  "//": "=========================================================",
  "//": "",
  "//": "IMPORTANT: Proxy and Fake SNI/IP optimization are mutually",
  "//": "           exclusive. When proxy is enabled (mode != 'off'),",
  "//": "           custom transport is automatically disabled to",
  "//": "           prevent conflicts.",
  "//": "",
  "//": "See: TECH_DESIGN_P5_PLAN.md for architectural details",
  "//": "",
  "//": "=========================================================",
  "//": "  Cross-Platform System Proxy Detection",
  "//": "=========================================================",
  "//": "",
  "//": "When mode = 'system', the application will detect proxy",
  "//": "settings from:",
  "//": "",
  "//": "Windows:",
  "//": "  - Registry: HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Internet Settings",
  "//": "  - Keys: ProxyEnable, ProxyServer",
  "//": "",
  "//": "macOS:",
  "//": "  - Command: scutil --proxy",
  "//": "  - Keys: HTTPProxy, HTTPPort, HTTPSProxy, HTTPSPort",
  "//": "",
  "//": "Linux:",
  "//": "  - Environment: HTTP_PROXY, HTTPS_PROXY",
  "//": "  - Precedence: HTTPS_PROXY > HTTP_PROXY",
  "//": "",
  "//": "========================================================="
}
